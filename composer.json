{
  "name": "xspkg/laravel-rabbitmq",
  "description": "采用laravel queue的方式监听 RabbitMQ 消息，完成 分布式 服务的通信功能。",
  "authors": [
    {
      "name": "兴顺",
      "email": "250915790@qq.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "illuminate/support": "5.5.*|5.6.*",
    "illuminate/queue": "5.5.*|5.6.*",
    "enqueue/amqp-lib": "0.9.*",
    "queue-interop/amqp-interop": "0.8.*"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*|6.*",
    "illuminate/events": "5.5.*|5.6.*"
  },
  "autoload": {
    "psr-4": {
      "XsPkg\\LaravelRabbitMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    },
    "laravel": {
      "providers": [
        "XsPkg\\LaravelRabbitMQ\\RabbitMQServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
