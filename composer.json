{
  "name": "xspkg/laravel-queue-rabbitmq",
  "description": "采用 RabbitMQ 结合 laravel 队列的监听，通过Job协作,完成 分布式 服务的通信功能。",
  "authors": [
    {
      "name": "兴顺",
      "email": "250915790@qq.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*",
    "illuminate/support": "5.7.*|5.8.*",
    "illuminate/queue": "5.7.*|5.8.*",
    "enqueue/amqp-lib": "0.9.*",
    "queue-interop/amqp-interop": "0.8.*"
  },
  "require-dev": {
    "illuminate/events": "5.7.*|5.8.*"
  },
  "autoload": {
    "psr-4": {
      "XsPkg\\LaravelQueueRabbitMQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "XsPkg\\LaravelQueueRabbitMQ\\QueueRabbitMQServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
